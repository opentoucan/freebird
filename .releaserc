{
  "branches": [
    "main",
    "+([0-9])?(.{+([0-9]),x}).x"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    [
      "@semantic-release/git",
      {
          "assets": [
              "Cargo.toml",
              "Cargo.lock"
          ]
      }
    ],
    [
      "semantic-release-cargo",
      {
        "check": false,
        "checkArgs": ["--no-deps"],
        "publish": false
      }
    ]
  ]
}
